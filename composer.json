{
    "name": "gomee/core",
    "description": "library for Laravel Web3",
    "authors": [
        {
            "name": "Doan LN",
            "email": "developer.vcc@gmail.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Gomee\\": "src/app/",
            "PHPHtmlParser\\": "src/PHPHtmlParser/"
        },
        "files": [
            "src/helpers/utils.php"
        ]
    },
    "require": {
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-zlib": "*",
        "guzzlehttp/guzzle": "7.x",
        "james-heinrich/getid3": "2.0.x-dev",
        "laravel/lumen-framework": "^10.0",
        "myclabs/php-enum": "^1.8",
        "paquettg/string-encode": "~1.0.0"
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.4-dev"
        },
        "laravel": {
            "providers": [
                "Gomee\\Providers\\CoreServiceProvider"
            ]
        }
    }
}
